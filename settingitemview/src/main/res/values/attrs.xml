<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="SettingItemView">
    <attr name="item_title" format="string" />
    <attr name="item_text_right" format="string" />
    <attr name="item_show_right" format="boolean" />
    <attr name="item_type">
      <enum name="nomal" value="0" />
      <enum name="cache" value="1" />
      <enum name="choose" value="2" />
    </attr>
  </declare-styleable>

  <declare-styleable name="CustomColorSwitchCompat">
    <attr name="toggleOnColor" format="color" />
    <attr name="toggleOffColor" format="color" />
    <attr name="bgOnColor" format="color" />
    <attr name="bgOffColor" format="color" />
  </declare-styleable>
</resources>